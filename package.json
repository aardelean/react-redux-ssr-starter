{
  "name": "er-prism-web",
  "version": "1.0.0",
  "description": "EverReal Digital Property Management",
  "main": "index.js",
  "author": "EverReal Gmbh <info@everreal.co>",
  "license": "MIT",
  "engines": {
    "node": "6.9.2"
  },
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\"",
    "start-prod": "node ./node_modules/better-npm-run start-prod",
    "build": "./node_modules/.bin/webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc ./src",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "jest --watch --coverage",
    "test-once": "jest --coverage"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 9300
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-proto-to-assign": "^6.22.0",
    "babel-plugin-transform-react-display-name": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "body-parser": "^1.16.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.15",
    "compression": "^1.6.2",
    "concurrently": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.0.1",
    "cors": "^2.8.1",
    "css-loader": "^0.26.1",
    "css-module-flow": "^1.0.0",
    "er-common-components": "^1.3.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "express": "^4.14.0",
    "express-session": "^1.15.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.38.0",
    "flowtype-loader": "^0.2.2",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack": "^0.0.4",
    "history": "3.0.0",
    "http": "^0.0.0",
    "imports-loader": "^0.7.0",
    "jquery": "^3.1.1",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "material-ui": "^0.16.7",
    "node-sass": "^4.3.0",
    "piping": "^1.0.0-rc.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-inline-css": "^2.3.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.3.3",
    "redux": "^3.6.0",
    "redux-connect": "^5.0.0",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-form": "^6.4.3",
    "redux-form-material-ui": "^4.1.2",
    "sass-loader": "^4.1.1",
    "scroll-behavior": "0.3.2",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.2",
    "serve-static": "^1.11.2",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "superagent": "^3.4.0",
    "url-loader": "^0.5.7",
    "watch": "^1.0.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-isomorphic-tools": "^2.6.6"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^18.1.0",
    "jest-enzyme": "^2.1.0",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.4.2"
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "src/universal/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleNameMapper": {}
  }
}
